<?xml version='1.0' encoding='UTF-8'?>
<hudson.tasks.Mailer_-DescriptorImpl plugin="mailer@1.16">
  <defaultSuffix>{{ jenkins_email_info.default_suffix }}</defaultSuffix>
  <replyToAddress>{{ jenkins_email_info.reply_to_address }}</replyToAddress>
  <smtpHost>{{ jenkins_email_info.smtp_host }}</smtpHost>
  <useSsl>{{ jenkins_email_info.use_ssl|lower }}</useSsl>
  <smtpPort>{{ jenkins_email_info.smtp_port }}</smtpPort>
  <charset>UTF-8</charset>
</hudson.tasks.Mailer_-DescriptorImpl>
